{
  "info": {"version": "1.0.0", "title": "App Launcher Helper API"},
  "paths": {
     "/rest/orgs/{organization_guid}/atkinstances": {
        "get": {
          "operationId": "get_atk_instances",
          "parameters": [{"name": "organization_guid", "in": "path", "required": "true", "type": "string"}],
          "responses": {
            "200": {"description": "OK", "schema": {"$ref": "ATKinstances"}}
        }
      }
     }
  },
  "definitions": {
    "ATKinstances": {
      "properties": {
        "instances": {
          "type": "array",
          "items": {"$ref": "InstancesInfo"}
        },
        "service_plan_guid": {"type": "string"}
      }
    },
    "InstancesInfo": {
      "properties": {
        "name": {"type": "string"},
        "url": {"type": "string"},
        "state": {"type": "string"}
      }
    }
  },
  "security": [{"default": []}],
  "swagger": "2.0",
  "consumes": ["application/json"]
}